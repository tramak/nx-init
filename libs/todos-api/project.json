{
  "root": "libs/todos-api",
  "sourceRoot": "libs/todos-api/src",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/todos-api",
        "main": "libs/todos-api/src/index.ts",
        "tsConfig": "libs/todos-api/tsconfig.lib.json",
        "assets": ["libs/todos-api/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/todos-api/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/todos-api"],
      "options": {
        "jestConfig": "libs/todos-api/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
